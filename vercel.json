{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/products",
      "dest": "index.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/(.+)",
      "dest": "index.html"
    }
  ]
}
